<div class="bg-white px-4 py-4">
    <div class="row g-3">
        <div class="form-section-header border-info d-flex justify-content-between">
            <div class="">
                 <h3 class="fw-bold text-primary">
                <i class="bi bi-mortarboard-fill fs-1 me-2"></i>Informations sur l'etudiant
            </h3>
            </div>
           
        </div>
    
        <!-- Civilité -->
        <div class="col-md-1">
            <p class="form-label fw-semibold">Civilite : </p> <span><%=frequentation.civilite  %> </span>
        </div>
    
        <!-- Nom -->
        <div class="col-md-3">
            <p class="form-label fw-semibold">Nom Complet : </p> <span><%=frequentation.prenom  %> <%=frequentation.nom  %> </span>
        </div>
    
        <!-- Filière -->
        <div class="col-md-2">
            <p class="form-label fw-semibold">Filiere : </p> <span > <%=frequentation.filiere  %> </span>
        </div>
    
        <!-- Niveau -->
        <div class="col-md-2">
            <p class="form-label fw-semibold">Niveau: </p> <span><%=frequentation.niveau %> </span>
        </div>
      <!-- Prénom -->
      <div class="col-md-2">
        <p class="form-label fw-semibold">Promotion: </p> <span><%=frequentation.promotion  %> </span>
    </div>
        <!-- Matricule -->
        <div class="col-md-2">
            <p class="form-label fw-semibold">Numéro matricule : </p> <span><%=frequentation.matricule  %> </span>
        </div>
    
    </div>
    <hr class="mx-4">
        

        <div class="form-section bg-white">
            <div class="form-section-header border-info">
                <h3 class="fw-bold text-primary">
                    <i class="bi bi-award fs-1 me-2"></i>Informations sur  demande d'Attestation
                </h3>
            </div>


            <div class=" d-flex justify-content-around">
               
                    <div class="col-md-4">
                        <p class="form-label fw-semibold">Id: </p> <span><%=frequentation.identifiant  %> </span>
                    </div>
                    <div class="mb-4">
                        <p class="form-label fw-semibold">Periode : </p> <span><%=frequentation.periode  %> </span>
                    </div>
                    <div class="mb-4">
                        <p class="form-label fw-semibold">Pour : </p> <span><%=frequentation.destination  %> </span>
                    </div>
            
            </div>
        </div>
</div>